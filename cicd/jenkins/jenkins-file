pipeline {
  agent any
  
  stages {
    stage('Build'){
      steps {
        sh 'echo "Building.."'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing...'
      }
    }
    stage('Push'){
      steps{
        sh 'echo push'
      }
    }
    stage('Deploy'){
      steps{
        echo 'Deploying...'
      }
    }
  }
}
